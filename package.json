{
  "name": "homebridge-ui",
  "version": "0.0.1",
  "description": "HomeBridge UI",
  "main": "server/app.js",
  "private": true,
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "develop": "concurrently \"npm run start\" \"tsc -w -p ./server\" \"nodemon ./server/bin/www\" ",
    "forever": "tsc -p ./server && forever start ./server/bin/www --watch",
    "twc:server:w": "tsc -w -p ./server",
    "typings": "typings install",
    "postinstall": "typings install && tsc -p ./server",
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "clean": "npm cache clean && rimraf -- node_modules doc typings coverage dist",
    "build": "export NODE_ENV=dev && webpack",
    "build:production": "export NODE_ENV=prod && webpack -p",
    "buildwin": "SET NODE_ENV=dev&&webpack",
    "buildwin:production": "SET NODE_ENV=prod&&webpack -p",
    "lint": "eslint src/ || true",
    "lint:travis": "eslint src/",
    "start": "webpack-dev-server --inline --port 3000 --content-base dist/",
    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  },
  "engines": {
    "node": ">= 5.4.1"
  },
  "author": "Nulled Labs",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "0.0.32",
    "@types/express": "^4.0.32",
    "@types/jsonwebtoken": "^7.1.31",
    "@types/serve-favicon": "^2.2.27",
    "angular": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "angular-schema-form": "^0.8.13",
    "angular-schema-form-bootstrap": "^0.2.0",
    "angular-ui-bootstrap": "^2.2.0",
    "angular-ui-router": "^0.3.1",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "forever": "^0.15.2",
    "jquery": "2.2.4",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.16.4",
    "npm": "*",
    "objectpath": "^1.2.1",
    "serve-favicon": "~2.3.0",
    "tv4": "^1.2.7",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.9.1",
    "angular-mocks": "1.5.8",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.6",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.4.1",
    "file-loader": "^0.9.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "html-webpack-plugin": "^2.24.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.15.0"
  }
}
